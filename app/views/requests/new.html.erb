<h1>New request</h1>

<% form_for(@request) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :askprice %><br />
    <%= f.text_field :askprice %>
  </p>
  <p>
    <%= f.label :description %><br />
    <%= f.text_area :description %>
  </p>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>